{
  "name": "file-explorer-svelte-boilerplate",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:cypress": "cypress run --component",
    "test:cypress:open": "cypress open --component",
    "test:cypress:watch": "cypress run --component --watch",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@cypress/svelte": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tsconfig/svelte": "^5.0.0",
    "@types/cypress": "^0.1.6",
    "cypress": "^14.5.1",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.2.6",
    "sass": "^1.77.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.5.0",
    "vite": "^5.4.4",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "type": "module"
}
